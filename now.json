{
  "version": 2,
  "name": "minelev-logs-stats",
  "alias": [
    "stats.logs.minelev.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_CONNECTION": "@db-connection",
    "MONGODB_COLLECTION": "logs"
  },
  "builds": [
    { "src": "/index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}