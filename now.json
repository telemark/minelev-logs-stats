{
  "version": 2,
  "name": "minelev-logs-stats",
  "scope": "minelev",
  "alias": [
    "stats.logs.minelev.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_CONNECTION": "@db-connection-read",
    "MONGODB_COLLECTION": "logs",
    "MONGODB_NAME": "@db-name"
  },
  "builds": [
    {
      "src": "/README.md",
      "use": "@now/md"
    },
    { 
      "src": "/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/README.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "/static/favicon.ico"
    },
    {
      "src": "/stats/(.*)",
      "dest": "/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept"
      }
    }
  ]
}