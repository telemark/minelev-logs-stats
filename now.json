{
  "version": 2,
  "name": "minelev-logs-stats",
  "scope": "minelev",
  "alias": [
    "stats.logs.minelev.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_CONNECTION": "@db-connection-read",
    "MONGODB_COLLECTION": "logs"
  },
  "builds": [
    { 
      "src": "/index.js",
      "use": "@now/node"
    },
    {
      "src": "/README.md",
      "use": "@now/md"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/README.html"},
    { "src": "/stats/(.*)", "dest": "/index.js" }
  ]
}